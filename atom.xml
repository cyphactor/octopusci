---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
 <title>OctopusCI</title>
 <link href="http://octopusci.info/atom.xml" rel="self"/>
 <link href="http://octopusci.info/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://octopusci.info/</id>
 <author>
   <name>OctopusCI</name>
   <email>adeponte@octopusci.info</email>
 </author>

 {% for post in site.posts %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://octopusci.info{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://octopusci.info{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}
 
</feed>
