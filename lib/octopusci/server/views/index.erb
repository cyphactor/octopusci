<% @jobs.each do |j| %>
  <div class="job">
    <div class="job_header">
      <div class="job_title">
        <a href="#"><%= j.repo_name %>/<%= j.branch_name %></a>
      </div>
      <div class="created_on">
        <%= j.created_at.strftime('%A, %B %d, %Y AT %I:%M%p') %>
      </div>
      <div style="clear: both;"></div>
    </div>
    <div class="job_content">
      
      Job: <%= j.id %></br>
      Finished: <%= j.ended_at.ago_in_words %></br>
      Duration: <%= j.ended_at - j.started_at %></br>
      Compare: <%= j.compare %></br>
      Pusher: <%= j.payload['pusher']['name'] %></br>   
    </div>
  </div>
<% end %>
